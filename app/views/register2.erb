<section class="SectionCards u-marginTopLarge">
  <form class="BoxRegister" id="LoginForm" action="/session" method="post" onload="onLoadLogin">

    <div class="LoginHeader">
      <h1 class="u-fontFamilyLato u-fontSizeMedium">Contato para negócios</h1>
    </div>

    <div class="BoxSteps">
      <a class="Button u-marginSmall u-widthSmall" href="/admin/register">1</a>
      <a class="Button u-marginSmall u-widthSmall" href="/admin/register/2">2</a>
      <a class="Button u-marginSmall u-widthSmall" href="/admin/register/3">3</a>
    </div>


    <div class="BoxRegisterBody">
      <div class="RegisterInputs">
        <span> Nome do contato:</span>
        <input type="text" name="" value="">
      </div>
      <div class="RegisterInputs">
        <span> E-mail:</span>
        <input type="text" name="" value="">
      </div>
      <div class="RegisterInputs">
        <span> E-mail alternativo:</span>
        <input type="text" name="" value="">
      </div>
      <div class="RegisterInputs">
        <span> Telefone:</span>
        <input type="text" name="" value="">
      </div>
      <div class="RegisterInputs">
        <span> Site:</span>
        <input type="text" name="" value="">
      </div>

      <div class="RegisterInputs">
        <span>Descrição:</span>
        <textarea> </textarea>
      </div>

    </div>

    <div class="RegisterFooter u-marginSidesAndBottom">
      <button class="Button" type="button" name="button">Próximo</button>
    </div>

  </form>
</section>
